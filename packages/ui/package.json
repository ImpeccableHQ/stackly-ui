{
  "name": "@stackly/ui",
  "version": "0.0.1",
  "repository": "https://github.com/ImpeccableHQ/stackly-ui",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "TIMING=1 eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
    "clean": "rm -rf .turbo"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./*": [
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*/index.ts"
    ],
    "./tailwind": {
      "require": "./tailwind.config.js"
    }
  },
  "files": [
    "src"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ]
    }
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "@swc/helpers": "^0.4.12",
    "autoprefixer": "^10.4.14",
    "clsx": "^1.1.1",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.2"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "@types/react": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.11.0",
    "eslint-plugin-react": "^7.32.2",
    "typescript": "4.8.4"
  },
  "description": "Stackly UI components",
  "main": "index.js"
}
